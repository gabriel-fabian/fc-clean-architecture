{
  "name": "fs-cleant-architecture",
  "version": "0.0.0",
  "description": "Node.js API using DDD, Clean Architecture and TypeScript.",
  "scripts": {
    "start": "echo \"Error: no script defined\" && exit 1",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand --no-cache",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "jest --runInBand --coverage",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.11",
    "@swc/jest": "^0.2.23",
    "@types/jest": "^29.2.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "git-commit-msg-linter": "^4.4.0",
    "husky": "^8.0.2",
    "jest": "^29.2.2",
    "lint-staged": "^13.1.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "author": "Gabriel Fabian",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.25.3",
